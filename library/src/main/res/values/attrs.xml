<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GWProgressView">
        <attr name="progress_stroke_color" format="color"/>         <!--描边颜色-->
        <attr name="progress_stroke_width" format="dimension"/>     <!--描边宽度-->
        <attr name="progress_radius" format="dimension"/>           <!--半径-->
        <attr name="progress_immediate" format="boolean"/>          <!--立刻执行-->
        <attr name="progress_duration" format="integer"/>           <!--持续时间-->
        <attr name="progress_interpolator" format="reference"/>     <!--差速器-->
        <attr name="progress_background_color" format="color"/>     <!--背景色-->

        <attr name="GWProgressViewStyle" format="reference"/>
    </declare-styleable>

</resources>
